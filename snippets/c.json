    "hpp_template": {
        "prefix": "hpptmp",
        "description": "A template for C++ Header Files",
        "body": [
            "//======================================================================================================================",
            "/// @file ${TM_FILENAME}",
            "///",
            "/// @brief ${1:Module description}",
            "///",
            "/// @par Copyright (c) ${CURRENT_YEAR} Rockwell Automation Technologies, Inc. All rights reserved.",
            "//======================================================================================================================",
            "",
            "#if !defined(${TM_FILENAME/(.*)\\..+$/${1:/upcase}/}_HPP)",
            "#define ${TM_FILENAME/(.*)\\..+$/${1:/upcase}/}_HPP",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Include files",
            "//----------------------------------------------------------------------------------------------------------------------",
            "",
            "// System includes",
            "// (none)",
            "",
            "// Project includes",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Namespace",
            "//----------------------------------------------------------------------------------------------------------------------",
            "namespace ${2:namespace}",
            "{",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Forward references",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Constants",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Macros",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Data types",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Forward declarations",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Function declarations",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "/// @brief ${3:class description}",
            "///",
            "/// ${4:class details}",
            "class ${5:classname}",
            "{",
            "public:",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // Class public constants.",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // (none)",
            "",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // Class public data type definitions.",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // (none)",
            "",
            "    /// NOTE: Use as needed",
            "    /// @brief No default constructor.",
            "    // Foo(void)                          = delete;",
            "    /// @brief No copy constructor.",
            "    // Foo(Foo const &rObject)            = delete;",
            "    /// @brief No move constructor.",
            "    // Foo(Foo &&rObject)                 = delete;",
            "    /// @brief No copy assignment operator.",
            "    // Foo &operator=(Foo const &rObject) = delete;",
            "    /// @brief No move assignment operator.",
            "    // Foo &operator=(Foo &&rObject)      = delete;",
            "",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // Class public member functions.",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // (none)",
            "",
            "protected:",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // Class protected data type definitions.",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // (none)",
            "",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // Class protected constants.",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // (none)",
            "",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // Class protected member variables.",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // (none)",
            "",
            "private:",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // Class private data type definitions.",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // (none)",
            "",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // Class private constants.",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // (none)",
            "",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // Class private member variables.",
            "    //------------------------------------------------------------------------------------------------------------------",
            "    // (none)",
            "",
            "};",
            "",
            "} // namespace",
            "",
            "#endif // #if !defined(${TM_FILENAME_BASE}_HPP)",
            "//======================================================================================================================",
            "// End of ${TM_FILENAME}",
            "// (note: the newline at the end of the file is intentional)",
            "//======================================================================================================================",
            ""
        ]
    },
    "h_template": {
        "prefix": "htmp",
        "description": "A template for C Header Files",
        "body": [
            "//======================================================================================================================",
            "/// @file ${TM_FILENAME}",
            "///",
            "/// @brief ${1:Module description}",
            "///",
            "/// @par Copyright (c) ${CURRENT_YEAR} Rockwell Automation Technologies, Inc. All rights reserved.",
            "//======================================================================================================================",
            "",
            "#if !defined(${TM_FILENAME/(.*)\\..+$/${1:/upcase}/}_HPP)",
            "#define ${TM_FILENAME/(.*)\\..+$/${1:/upcase}/}_HPP",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Include files",
            "//----------------------------------------------------------------------------------------------------------------------",
            "",
            "// System includes",
            "// (none)",
            "",
            "// Project includes",
            "// (none)",
            "",
            "// For inclusion in C++ files, declare everything extern \"C\".",
            "#if defined( __cplusplus )",
            "extern \"C\" {",
            "#endif",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Constants",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Macros",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Forward function declarations",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Data types",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Variable definitions",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// Function declarations",
            "//----------------------------------------------------------------------------------------------------------------------",
            "// (none)",
            "",
            "// End of extern \"C\".",
            "#if defined( __cplusplus )",
            "}",
            "#endif",
            "",
            "#endif // #if !defined(${TM_FILENAME_BASE}_HPP)",
            "//======================================================================================================================",
            "// End of ${TM_FILENAME}",
            "// (note: the newline at the end of the file is intentional)",
            "//======================================================================================================================",
            ""
        ]
    },
    "function_block": {
        "prefix": "fblock",
        "description": "A C/C++ function comment block",
        "body": [
            "/// @brief ${1:description}",
            "///",
            "/// @param ${2:parameters}",
            "///",
            "/// @return ${3:return}",
            "/// @retval ${4:return values}"
        ]
    }
